<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="deedeveloper.examples.va_classifier" >

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity android:name=".User_Management.MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".User_Management.Login" >
            <intent-filter>
                <action android:name="deedeveloper.examples.va_classifier.Login" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".User_Management.UserProfile" >
            <intent-filter>
                <action android:name="deedeveloper.examples.va_classifier.UserProfile" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".User_Management.EditUser" >
            <intent-filter>
                <action android:name="android.intent.action.EditUser" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".User_Management.RootActivity" />
        <activity android:name=".User_Management.ClassificationPage" />
        <activity android:name=".User_Management.HistoryPage" />
        <activity android:name=".User_Management.Logout" />
        <activity android:name=".LoadData" >
        </activity>
        <activity android:name=".ECG_output" >
        </activity>
    </application>

</manifest>
